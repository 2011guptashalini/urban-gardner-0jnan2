# Base URL for backend API endpoints
VITE_API_BASE_URL=http://localhost:8080/api/v1

# API request timeout in milliseconds (30 seconds)
VITE_API_TIMEOUT=30000

# Authentication Configuration
# JWT token expiry in seconds (1 hour)
VITE_AUTH_TOKEN_EXPIRY=3600

# Refresh token expiry in seconds (7 days)
VITE_REFRESH_TOKEN_EXPIRY=604800

# Application Configuration
# Application name for display and metadata
VITE_APP_NAME=Urban Gardening Assistant

# Application version for versioning and updates
VITE_APP_VERSION=1.0.0

# Environment name (development/staging/production)
VITE_NODE_ENV=development